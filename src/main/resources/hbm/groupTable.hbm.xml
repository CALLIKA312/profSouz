<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.GroupModel" table="group" optimistic-lock="version">

        <id name="groupCode" type="java.lang.String"/>
        <property name="facultyName" type="java.lang.String"/>
        <property name="groupName" type="java.lang.String"/>

    </class>
</hibernate-mapping>